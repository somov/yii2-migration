{
  "name": "somov/yii2-migration",
  "description": "Yii2 migration with suffix.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "common",
     "migration"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Somov Nikolay",
      "email": "somov.nn@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": ">=2.0.16"
  },

  "require-dev": {
    "somov/common": ">=1.1.1"
  },

  "autoload": {
    "psr-4": {
      "somov\\migration\\": "src"
    }
  },
  "archive": {
    "exclude": [
      "/.*",
      "/tests",
      "/vendor",
      "/bin"
    ]
  }
}
